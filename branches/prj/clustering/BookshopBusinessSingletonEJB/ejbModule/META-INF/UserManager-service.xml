<?xml version="1.0" encoding="UTF-8"?>
<server>
	<mbean
		name="g8.bookshop.business.um:service=UserManager"
		code="g8.bookshop.business.um.UserManager">
	</mbean>
	<mbean
		name="g8.bookshop.business.um:service=UserManagerController"
		code="org.jboss.ha.singleton.HASingletonController">
		<depends
			optional-attribute-name = "ClusterPartition"
			proxy-type = "attribute">
			jboss:service=HAPartition,partition=G8Business
		</depends>
		<depends>g8.test:service=Prova</depends>
		<depends>g8.bookshop.business.um:service=UserManager</depends>
		<attribute name="TargetName">g8.bookshop.business.um:service=UserManager</attribute>
		<attribute name="TargetStartMethod">startSingleton</attribute>
		<attribute name="TargetStopMethod">stopSingleton</attribute>
   </mbean>
</server>
